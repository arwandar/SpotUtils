{
  "name": "spotutils",
  "version": "2.0.0",
  "description": "Expack is the bare-bones Express and Webpack boilerplate with ES6+ babel transpilation, ESLint linting, Hot Module Reloading.",
  "main": "index.js",
  "scripts": {
    "buildDev": "rm -rf dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "buildDevW": "del /s /q dist && webpack --mode development --config webpack.server.config.js && webpack --mode development --config webpack.dev.config.js",
    "buildProd": "rm -rf dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "buildProdW": "del /s /q dist && webpack --mode production --config webpack.server.config.js && webpack --mode production --config webpack.prod.config.js",
    "start": "node ./dist/server.js",
    "startDev": "yarn buildDevW && yarn start",
    "eslint-fix": "eslint --ext .jsx,.js ./src --fix --config .eslintrc",
    "prettier": "prettier --write src/**/*.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "add": "^2.0.6",
    "axios": "0.18.0",
    "bootstrap": "4.2.1",
    "empty": "0.10.1",
    "express": "4.16.4",
    "i18next": "13.1.4",
    "immutable": "4.0.0-rc.12",
    "moment": "2.23.0",
    "node-persist": "3.0.3",
    "query-string": "6.2.0",
    "random-js": "1.0.8",
    "react": "16.7.0",
    "react-dom": "16.7.0",
    "react-i18next": "9.0.4",
    "react-redux": "6.0.0",
    "reactstrap": "7.1.0",
    "redux": "4.0.1",
    "redux-actions": "2.6.4",
    "redux-immutable": "4.0.0",
    "redux-thunk": "2.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-class-properties": "7.2.3",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "css-loader": "2.1.0",
    "eslint": "5.12.1",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.6.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.4",
    "file-loader": "3.0.1",
    "flow-bin": "0.91.0",
    "flow-typed": "2.5.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "less": "3.9.0",
    "less-loader": "4.1.0",
    "mini-css-extract-plugin": "0.5.0",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "prettier": "1.16.0",
    "style-loader": "0.23.1",
    "uglifyjs-webpack-plugin": "2.1.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.4",
    "webpack-cli": "3.2.1",
    "webpack-dev-middleware": "3.5.1",
    "webpack-hot-middleware": "2.24.3",
    "webpack-node-externals": "1.7.2"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "arrowParens": "always",
    "printWidth": 100,
    "trailingComma": "es5"
  }
}
